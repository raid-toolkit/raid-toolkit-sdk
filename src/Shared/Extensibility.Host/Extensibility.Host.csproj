<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0-windows</TargetFramework>
        <LangVersion>10.0</LangVersion>
        <AssemblyName>Raid.Toolkit.Extensibility.Host</AssemblyName>
        <RootNamespace>Raid.Toolkit.Extensibility</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="il2cpptoolkit.common" Version="1.0.59" />
        <PackageReference Include="il2cpptoolkit.metadata" Version="1.0.59" />
        <PackageReference Include="il2cpptoolkit.reversecompiler" Version="1.0.59" />
        <PackageReference Include="il2cpptoolkit.runtime" Version="1.0.59" />
        <PackageReference Include="il2cpptoolkit.target.netcore" Version="1.0.59" />
        <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="6.0.0" />
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.0" />
        <Reference Include="Raid.Interop.dll">
            <HintPath>$(BinDir)$(TargetFrameworkVersion)\Raid.Interop.dll</HintPath>
            <Private>False</Private>
            <SpecificVersion>False</SpecificVersion>
            <CopyLocal>False</CopyLocal>
        </Reference>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Model\Model.csproj" />
        <ProjectReference Include="..\Extensibility\Extensibility.csproj" />
    </ItemGroup>

</Project>
