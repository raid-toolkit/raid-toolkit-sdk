<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net6.0-windows</TargetFrameworks>
        <LangVersion>10.0</LangVersion>
        <developmentDependency>true</developmentDependency>
        <NoPackageAnalysis>true</NoPackageAnalysis>
        <IncludeBuildOutput>false</IncludeBuildOutput>
        <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
        <AssemblyName>Raid.Toolkit.Extensibility.Tasks</AssemblyName>
        <RootNamespace>Raid.Toolkit.Extensibility.Tasks</RootNamespace>
        <Nullable>enable</Nullable>
        <Platforms>x64</Platforms>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Build.Tasks.Core" Version="17.1.0" />
        <PackageReference Include="il2cpptoolkit.common" Version="2.0.114-alpha" />
        <PackageReference Include="il2cpptoolkit.metadata" Version="2.0.114-alpha" />
        <PackageReference Include="il2cpptoolkit.reversecompiler" Version="2.0.114-alpha" />
        <PackageReference Include="il2cpptoolkit.runtime" Version="2.0.114-alpha" />
        <PackageReference Include="il2cpptoolkit.target.netcore" Version="2.0.114-alpha" />
        <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
        <PackageReference Include="MSBuildTasks" Version="1.5.0.235">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Model\Model.csproj" />
        <ProjectReference Include="..\Common\Common.csproj" />
    </ItemGroup>

    <ItemGroup>
        <None Include="build\**">
            <Pack>true</Pack>
            <PackagePath>build\</PackagePath>
        </None>
        <None Include="buildCrossTargeting\**">
            <Pack>true</Pack>
            <PackagePath>buildCrossTargeting\</PackagePath>
        </None>
        <!-- required for tasks -->
