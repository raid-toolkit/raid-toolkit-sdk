<Project Sdk="Microsoft.NET.Sdk">

    <ItemGroup>
        <PackageReference Include="il2cpptoolkit.common"
            Version="2.0.48-alpha" />
        <PackageReference Include="il2cpptoolkit.metadata"
            Version="2.0.48-alpha" />
        <PackageReference Include="il2cpptoolkit.reversecompiler"
            Version="2.0.48-alpha" />
        <PackageReference Include="il2cpptoolkit.runtime"
            Version="2.0.48-alpha" />
        <PackageReference Include="il2cpptoolkit.target.netcore"
            Version="2.0.48-alpha" />
        <PackageReference Include="Microsoft.Win32.Registry"
            Version="5.0.0" />
        <PackageReference Include="Newtonsoft.Json"
            Version="13.0.1" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Common\Common.csproj" />
    </ItemGroup>

    <ItemGroup>
        <Content Include="$(BinDir)v4.7.2\Raid.Interop.dll"
            copyToOutput="true">
            <Pack>true</Pack>
            <PackagePath>lib\net472</PackagePath>
        </Content>
        <Content Include="$(BinDir)v5.0\Raid.Interop.dll"
            copyToOutput="true">
            <Pack>true</Pack>
            <PackagePath>lib\net5.0-windows7.0</PackagePath>
        </Content>
    </ItemGroup>

    <PropertyGroup>
        <TargetFrameworks>net472;net5.0-windows;net6.0-windows</TargetFrameworks>
        <LangVersion>10.0</LangVersion>
        <OutputType>Library</OutputType>
        <AssemblyName>Raid.Model</AssemblyName>
        <RootNamespace>Raid.Model</RootNamespace>
        <Platforms>x64</Platforms>
    </PropertyGroup>

</Project>
